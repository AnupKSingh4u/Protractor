{
  "name": "ProtractorFramework",
  "dependencies": {
    "jasmine-data-provider": "2.2.0",
    "karma": "3.1.4",
    "karma-cli": "latest",
    "karma-jasmine": "latest",
    "karma-phantomjs-launcher--save-dev": "latest",
    "karma-requirejs": "^1.1.0",
    "protractor": "^5.3.0",
    "protractor-jasmine2-html-reporter": "0.0.7"
  },
  "scripts": {
    "webdriver-update": "./node_modules/.bin/webdriver-manager update",
    "webdriver-start": "./node_modules/.bin/webdriver-manager start",
    "protractor": "./node_modules/.bin/protractor config.js",
    "start": "npm run webdriver-update && npm run webdriver-start",
    "Smoke": "./node_modules/.bin/protractor config.js --suite Smoke",
    "Regression": "./node_modules/.bin/protractor config.js --suite Regression",
    "Separator1": "**********************************************************************************************************",
    "Separator2": "**********************************************************************************************************",
    "Add": "git add *",
    "commit": "git commit -m 'Commit message'",
    "push-master": "git push origin master",
    "push-develop": "git push origin develop",
    "status": "git status"
  },
  "devDependencies": {
    "phantomjs": "^2.1.7"
  }
}
